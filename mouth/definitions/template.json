{
	"__name__": "Mouth_Template",

	"_id": {
		"__type__": "uuid",
		"__optional__": true
	},

	"_created": {
		"__type__": "timestamp",
		"__optional__": true
	},

	"_updated": {
		"__type__": "timestamp",
		"__optional__": true
	},

	"name": {
		"__type__": "string",
		"__maximum__": "64"
	},

	"variables": {
		"__hash__": {
			"__type__": "string",
			"__regex__": "^[a-z_]+$"
		},
		"__type__": "string"
	},

	"locales": {
		"__hash__": {
			"__type__": "string",
			"__regex__": "^[a-z]{2}-[A-Z]{2}$"
		},
		"__type__": {
			"__hash__": {
				"__type__": "string",
				"__regex__": "^[a-z]{32}$"
			},
			"__type__": "string",
			"__maximum__": 65535
		}
	}
}